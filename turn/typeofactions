
	Meteor Spell {
		performer: 		[Sephiroth]
		targets: 		[ _aliveG || _aliveB ]
		reflectable: 	[false]
		counterable: 	[false]

		setGrahicEfx: 	function(target, player, callback){ console.log('Loading action starting'); callback(); },
		checkMiss: 		function(target, player, callback){ console.log('Check on: ', target.Name); callback(); },
		playGraphics: 	function(target, player, callback){ console.log('Play damage on: ', target.Name); callback(); },
		calcDamages: 	function(target, player, callback){ console.log('Calc damage on: ', target.Name); callback(); },
		showDamages: 	function(target, player, callback){ console.log('Show damage on: ', target.Name); callback(); },
		end	: 			function(target, player, callback){ console.log('Play on: ', target.Name); callback(); },
	}

